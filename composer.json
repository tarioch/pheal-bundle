{
    "name": "tarioch/pheal-bundle",
    "description": "Adds Pheal support to your Symfony2 project.",
    "keywords": ["Pheal", "PhealNG", "EVE Online", "EVE API", "Symfony"],
    "homepage": "https://github.com/tarioch/pheal-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
		"name": "Talos Katuma",
		"homepage": "https://hplcorp.net"
        }
    ],
    "support": {
	"issues": "https://github.com/tarioch/pheal-bundle/issues"
    },
    "require": {
        "php": ">=5.4.4",
		"symfony/framework-bundle": ">=2.3",
		"jms/di-extra-bundle" : ">=1.4",
		"3rdpartyeve/phealng" : ">=2.3"
    },
    "require-dev": {
	"phpunit/phpunit" : "3.7.*",
	"mockery/mockery" : "dev-master@dev"
    },
    "autoload": {
	"psr-0": { "Tarioch\\PhealBundle": "" }
    },
    "target-dir": "Tarioch/PhealBundle"
}
